{
	"name": "test-monorepo-final",
	"version": "0.1.0",
	"private": true,
	"workspaces": ["apps/*", "packages/*"],
	"scripts": {
		"build": "bunx --bun @moonrepo/cli run :build",
		"build:web": "bunx --bun @moonrepo/cli run web:build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"clean": "bunx --bun @moonrepo/cli clean cache",
		"dev": "bunx --bun @moonrepo/cli run :dev",
		"dev:expo": "cd apps/expo && bun run dev",
		"dev:web": "bunx --bun @moonrepo/cli run web:dev",
		"format": "bunx --bun @moonrepo/cli run :format",
		"lint": "bunx --bun @moonrepo/cli run :lint",
		"typecheck": "bunx --bun @moonrepo/cli run :typecheck",
		"with-env": "dotenv -e .env --"
	},
	"dependencies": {},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@moonrepo/cli": "^1.32.0",
		"@types/node": "^20.14.10",
		"dotenv-cli": "^8.0.0",
		"typescript": "^5.8.2"
	},
	"packageManager": "bun@1.1.0",
	"engines": {
		"node": ">=20.0.0"
	},
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	}
}
